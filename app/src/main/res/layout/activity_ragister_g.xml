<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Ragister_G">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="260px"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="1300px"
                android:layout_marginLeft="1300px"
                android:layout_marginTop="130px"
                android:text="완료"
                android:textColor="#000000"
                android:textSize="45px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="128px"
                android:text="등록하기"
                android:textColor="#000000"
                android:textSize="74px" />
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="324px"
        android:background="#F7F7F7"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <EditText
                android:id="@+id/editTextTextPersonName2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="58px"
                android:layout_marginLeft="58px"
                android:layout_marginTop="147px"
                android:layout_marginEnd="58px"
                android:layout_marginRight="58px"
                android:ems="10"
                android:gravity="right"
                android:inputType="textPersonName"
                android:text="000-0000-0000"
                android:textSize="50px" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="58px"
                android:layout_marginLeft="58px"
                android:layout_marginTop="65px"
                android:text="보호자 등록"
                android:textColor="#000000"
                android:textSize="55px" />
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1976px"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent">

        <LinearLayout
            android:layout_width="1324px"
            android:layout_height="220px"
            android:layout_marginStart="58px"
            android:layout_marginLeft="58px"
            android:layout_marginTop="124px"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="보호자 이름"
                    android:textColor="#000000"
                    android:textSize="55px" />

                <EditText
                    android:id="@+id/editTextTextPersonName3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="72px"
                    android:ems="10"
                    android:gravity="right"
                    android:inputType="textPersonName"
                    android:text="홍길동"
                    android:textSize="50px" />
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="1324px"
            android:layout_height="247px"
            android:layout_marginStart="58px"
            android:layout_marginLeft="58px"
            android:layout_marginTop="86px"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RadioButton
                    android:id="@+id/radioButton5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="700px"
                    android:layout_marginLeft="700px"
                    android:layout_marginTop="127px"
                    android:text="배우자"
                    android:textSize="45px" />

                <RadioButton
                    android:id="@+id/radioButton4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="127px"
                    android:text="형제/자매"
                    android:textSize="45px" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="관계"
                    android:textColor="#000000"
                    android:textSize="55px" />

                <RadioButton
                    android:id="@+id/radioButton3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="400px"
                    android:layout_marginLeft="400px"
                    android:layout_marginTop="127px"
                    android:text="부모님"
                    android:textSize="45px" />
            </FrameLayout>
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>